{
    "name": "ocikyamin/simpsb",
    "description": "Sistem Penerimaan Santri Baru",
    "license": "MIT",
    "type": "project",
    "homepage": "https://github.com/ocikyamin/SIMPSB-BSI",
    "support": {
        "forum": "https://github.com/ocikyamin/SIMPSB-BSI",
        "source": "https://github.com/ocikyamin/SIMPSB-BSI"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.0",
        "picqer/php-barcode-generator": "^2.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
